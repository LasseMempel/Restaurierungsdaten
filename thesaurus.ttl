Beispiel: 2025-05-13_RFA_1 oder XY_24601"""@de ;
    skos:editorialNote "Text" ;
    skos:inScheme <https://www.lassemempel.github.io/Restaurierungsdaten/> ;
    skos:notation "C421BF" ;
    skos:prefLabel "Untersuchungskennung"@de ;
