        "Parameter"@de ;
    skos:broader <https://www.lassemempel.github.io/Restaurierungsdaten//C93638> ;
    skos:definition "Angaben zu den Einstellungen bzw. Metadaten des Gerätes, welches für die Untersuchung verwendet wurde."@de ;
    skos:inScheme <https://www.lassemempel.github.io/Restaurierungsdaten/> ;
    skos:notation "C9C527" ;
    skos:prefLabel "Geräteeinstellungen (Untersuchungsgerät)"@de ;
    skos:scopeNote "empfohlen"@de .

<https://www.lassemempel.github.io/Restaurierungsdaten//CA1BC5> a skos:Concept ;
    skos:altLabel "Befund"@de ;
    skos:broader <https://www.lassemempel.github.io/Restaurierungsdaten//C2488A> ;
    skos:definition "Angabe, der bei der Untersuchung festgestellten Beobachtungen und Ergebnisse."@de ;
    skos:inScheme <https://www.lassemempel.github.io/Restaurierungsdaten/> ;
