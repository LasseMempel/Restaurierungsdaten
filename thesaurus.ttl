    skos:broader <https://www.lassemempel.github.io/Restaurierungsdaten//C33D85> ;
    skos:definition "Datum, an dem die jeweilige Untersuchung durchgeführt wurde."@de ;
    skos:editorialNote "Text (Datum)" ;
    skos:inScheme <https://www.lassemempel.github.io/Restaurierungsdaten/> ;
